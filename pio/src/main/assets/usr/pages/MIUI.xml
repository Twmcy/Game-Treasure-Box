        <set>. ./Game_Toolbox_Delete.sh</set>
            <param name="package" type="app" multiple="true" options-sh="cat $Game_Toolbox_File|sed '/^Han$/d'" value-sh="cat $Game_Toolbox_File|sed '/^Han$/d'" />
    </action>
<!-- END -->
